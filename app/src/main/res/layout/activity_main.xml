<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <FrameLayout
        android:id="@+id/parent_figma_prompt_drive_now_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/parent_figma_prompt_drive_now_app">

        <LinearLayout
            android:id="@+id/container_main"
            android:layout_width="345.3dp"
            android:layout_height="239.99dp"
            android:layout_marginLeft="23.99dp"
            android:layout_marginTop="244.18dp"
            android:layout_gravity="top|left"
            android:gravity="left|top"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/container_icon"
                android:layout_width="95.99dp"
                android:layout_height="95.99dp"
                android:layout_marginLeft="124.66dp"
                android:layout_gravity="left"
                android:background="@drawable/container_icon"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingRight="0.02dp">

                <ImageView
                    android:id="@+id/icon_drive_now"
                    android:layout_width="47.99dp"
                    android:layout_height="47.99dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/icon_drive_now" />
            </LinearLayout>

            <TextView
                android:id="@+id/text_heading_drive_anywhere"
                android:layout_width="match_parent"
                android:layout_height="32.01dp"
                android:layout_marginTop="47.99dp"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:gravity="center_horizontal|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Drive Anywhere"
                android:textColor="#FFFFFFFF"
                android:textSize="24dp" />

            <TextView
                android:id="@+id/container_paragraph"
                android:layout_width="332dp"
                android:layout_height="48dp"
                android:layout_marginLeft="6.97dp"
                android:layout_marginTop="12.99dp"
                android:layout_marginBottom="3.01dp"
                android:layout_gravity="left"
                android:layout_weight="1"
                android:gravity="center_horizontal|top"
                android:includeFontPadding="false"
                android:lineHeight="24dp"
                android:text="Pick up your car from convenient locations and enjoy the freedom of the road."
                android:textColor="#FFB0B0B0"
                android:textSize="16dp" />
        </LinearLayout>

    <LinearLayout
        android:id="@+id/container_buttons"
        android:layout_width="393.28dp"
        android:layout_height="123.95dp"
        android:layout_gravity="top|left"
        android:layout_marginTop="580.36dp"
        android:gravity="left|top"
        android:orientation="vertical"
        android:paddingLeft="23.99dp"
        android:paddingRight="23.99dp">

        <LinearLayout
            android:id="@+id/container_images"
            android:layout_width="match_parent"
            android:layout_height="7.98dp"
            android:gravity="center_horizontal|top"
            android:orientation="horizontal"
            android:paddingLeft="0dp">

            <ImageView
                android:id="@+id/image_container"
                android:layout_width="7.98dp"
                android:layout_height="7.98dp"
                android:background="@drawable/image_container"
                android:scaleType="centerInside" />

            <ImageView
                android:id="@+id/image_container1"
                android:layout_width="7.98dp"
                android:layout_height="7.98dp"
                android:layout_marginLeft="7.98dp"
                android:background="@drawable/image_container1"
                android:scaleType="centerInside" />

            <ImageView
                android:id="@+id/image_container2"
                android:layout_width="7.98dp"
                android:layout_height="7.98dp"
                android:layout_marginLeft="7.98dp"
                android:background="@drawable/image_container2"
                android:scaleType="centerInside" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_action_buttons"
            android:layout_width="match_parent"
            android:layout_height="35.98dp"
            android:layout_marginTop="31.99dp"
            android:gravity="left|center_vertical"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/button_skip"
                android:layout_width="59.04dp"
                android:layout_height="35.98dp"
                android:background="@drawable/button_skip"
                android:clipToPadding="false"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="16dp"
                android:paddingTop="7.99dp"
                android:paddingRight="16dp"
                android:paddingBottom="7.99dp">

                <TextView
                    android:id="@+id/text_skip"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Skip"
                    android:textColor="#FFB0B0B0"
                    android:textSize="14dp" />
            </LinearLayout>
            <!-- 1:65:space-view0 Container_space_view0 space-view -->
            <View
                android:layout_width="0dp"
                android:layout_height="35.98dp"
                android:layout_weight="1" />

            <FrameLayout
                android:id="@+id/button_get_started"
                android:layout_width="128.06dp"
                android:layout_height="35.98dp"
                android:background="@drawable/button_get_started">

                <TextView
                    android:id="@+id/text_get_started"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="11.99dp"
                    android:layout_marginBottom="0.32dp"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Get Started"
                    android:textColor="#FFFFFFFF"
                    android:textSize="14dp" />

                <ImageView
                    android:id="@+id/icon_get_started"
                    android:layout_width="15.98dp"
                    android:layout_height="15.98dp"
                    android:layout_gravity="top|left"
                    android:layout_marginLeft="100.08dp"
                    android:layout_marginTop="10dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/icon_get_started" />
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
</ScrollView>